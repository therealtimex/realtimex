{
  "$schema": "../node_modules/@tauri-apps/cli/config.schema.json",
  "productName": "RealTimeX AI",
  "version": "0.1.2",
  "identifier": "co.realtimex.ai",
  "build": {
    "frontendDist": "../dist",
    "devUrl": "http://127.0.0.1:3000",
    "beforeDevCommand": "yarn dev",
    "beforeBuildCommand": "yarn build"
  },
  "app": {
    "windows": [
      {
        "title": "RealTimeX AI",
        "width": 1280,
        "height": 720,
        "resizable": true,
        "fullscreen": false
      }
    ],
    "security": {
      "csp": null
    }
  },
  "bundle": {
    "active": true,
    "targets": "all",
    "icon": [
      "icons/32x32.png",
      "icons/128x128.png",
      "icons/128x128@2x.png",
      "icons/icon.icns",
      "icons/icon.ico"
    ],
    "externalBin": [
      "binaries/redis-server",
      "binaries/start-app"
    ],
    "resources": {
      "../../server":"server",
      "../../realtimex-ai-server":"realtimex-ai-server",
      "resources":"resources"
    }
  }
}
